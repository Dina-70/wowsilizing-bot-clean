# Telegram Bot Token (получите у @BotFather)
BOT_TOKEN=your_bot_token_here

# Admin ID (ваш Telegram user ID)
ADMIN_ID=123456789

# OpenAI API Key (опционально)
OPENAI_API_KEY=your_openai_key_here

# AI Model (опционально)
AI_MODEL=gpt-3.5-turbo

# Database URL (опционально, по умолчанию SQLite)
DATABASE_URL=sqlite:///bot.db

# Maximum video size in MB (опционально)
MAX_VIDEO_SIZE_MB=50

# Log level (опционально: DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO
